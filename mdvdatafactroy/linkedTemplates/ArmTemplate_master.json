{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "mdvdatafactroy"
		},
		"AzureSqlDatabase1_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureSqlDatabase1'"
		},
		"LinkCopyFile_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'LinkCopyFile'"
		},
		"LinkCopyFile_properties_typeProperties_host": {
			"type": "string",
			"defaultValue": "\\\\LAPTOP-PR62F6VD\\Users\\madha\\OneDrive\\Desktop\\Learning\\ADF"
		},
		"LinkCopyFile_properties_typeProperties_userId": {
			"type": "string",
			"defaultValue": "madhav.parashar@gmail.com"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "mdvdatafactroy_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureSqlDatabase1_connectionString": {
						"value": "[parameters('AzureSqlDatabase1_connectionString')]"
					},
					"LinkCopyFile_password": {
						"value": "[parameters('LinkCopyFile_password')]"
					},
					"LinkCopyFile_properties_typeProperties_host": {
						"value": "[parameters('LinkCopyFile_properties_typeProperties_host')]"
					},
					"LinkCopyFile_properties_typeProperties_userId": {
						"value": "[parameters('LinkCopyFile_properties_typeProperties_userId')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}